<div class ="play_again_content">
  <h1> MMORPS </h1>
  <h2> Scorebord </h2>

  <table>
    <tr>
      <th>Player</th>
      <th>Computer</th>
    </tr>
    <tr>
      <td><%= session[:player_score] %></td>
      <td><%= session[:computer_score] %></td>
    </tr>
  </table>

  <h4 class="cpu"> CPU THREW <%=session[:cpu_output]%></h4>
  <h4 class="player"> YOU THREW <%=session[:player_choice]%></h4>
  <h4> <%=session[:game_winner]%> WINS GAME!</h4>

  <form class="" action="/reset" method="post">
    <input class="button round" type="Submit" name="reset" value="Play Again">
  </form>
</div>
